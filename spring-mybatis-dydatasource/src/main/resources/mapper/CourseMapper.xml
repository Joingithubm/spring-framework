<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.athome.mapper.CourseMapper">
    <select id="selectAll" resultType="map">
        select * from course_1
    </select>

    <insert id="insert" parameterType="com.athome.entity.CourseEntity">
        insert into course_1(cid,cname,userId,cstatus) values (${cid},${cname},${cuserId},${cstatus})
    </insert>
</mapper>
